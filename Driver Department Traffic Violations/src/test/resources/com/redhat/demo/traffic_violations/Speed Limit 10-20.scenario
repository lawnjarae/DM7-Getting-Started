<scenario>
  <name>Speed Limit 10-20.scenario</name>
  <maxRuleFirings>100000</maxRuleFirings>
  <globals/>
  <fixtures>
    <fact-data>
      <type>Violation</type>
      <fieldData>
        <field-data>
          <name>speedLimit</name>
          <value>40</value>
          <nature>4</nature>
        </field-data>
        <field-data>
          <name>type</name>
          <value>Speed</value>
          <nature>4</nature>
        </field-data>
        <field-data>
          <name>actualSpeed</name>
          <value>55</value>
          <nature>4</nature>
        </field-data>
      </fieldData>
      <name>violation</name>
      <isModify>false</isModify>
    </fact-data>
    <org.drools.workbench.models.testscenarios.shared.ActivateRuleFlowGroup>
      <name>trafficViolation</name>
    </org.drools.workbench.models.testscenarios.shared.ActivateRuleFlowGroup>
    <execution-trace/>
    <expect-fact>
      <fieldValues>
        <expect-field>
          <fieldName>fineAmount</fieldName>
          <expected>100.0</expected>
          <operator>==</operator>
          <nature>0</nature>
        </expect-field>
        <expect-field>
          <fieldName>points</fieldName>
          <expected>1</expected>
          <operator>==</operator>
          <nature>0</nature>
        </expect-field>
      </fieldValues>
      <name>violation</name>
      <anonymous>false</anonymous>
    </expect-fact>
    <expect-rule>
      <ruleName>Row 1 SpeedViolationRule</ruleName>
      <expectedFire>true</expectedFire>
    </expect-rule>
  </fixtures>
  <rules/>
  <inclusive>false</inclusive>
  <packageName>com.redhat.demo.traffic_violations</packageName>
  <imports>
    <imports/>
  </imports>
  <ksessions>
    <string>default-stateful-kie-session</string>
  </ksessions>
</scenario>