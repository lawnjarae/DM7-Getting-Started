<scenario>
  <name>Suspend due to multiple violations.scenario</name>
  <maxRuleFirings>100000</maxRuleFirings>
  <globals/>
  <fixtures>
    <fact-data>
      <type>Driver</type>
      <fieldData>
        <field-data>
          <name>totalPoints</name>
          <value>5</value>
          <nature>4</nature>
        </field-data>
      </fieldData>
      <name>driver</name>
      <isModify>false</isModify>
    </fact-data>
    <fact-data>
      <type>Violation</type>
      <fieldData>
        <field-data>
          <name>points</name>
          <value>10</value>
          <nature>4</nature>
        </field-data>
      </fieldData>
      <name>violation</name>
      <isModify>false</isModify>
    </fact-data>
    <org.drools.workbench.models.testscenarios.shared.ActivateRuleFlowGroup>
      <name>trafficViolation</name>
    </org.drools.workbench.models.testscenarios.shared.ActivateRuleFlowGroup>
    <fact-data>
      <type>Violation</type>
      <fieldData>
        <field-data>
          <name>points</name>
          <value>5</value>
          <nature>4</nature>
        </field-data>
      </fieldData>
      <name>violation2</name>
      <isModify>false</isModify>
    </fact-data>
    <execution-trace/>
    <expect-rule>
      <ruleName>DriverLicenseSuspensionRule</ruleName>
      <expectedFire>true</expectedFire>
    </expect-rule>
    <expect-fact>
      <fieldValues>
        <expect-field>
          <fieldName>state</fieldName>
          <expected>suspend</expected>
          <operator>==</operator>
          <nature>1</nature>
        </expect-field>
      </fieldValues>
      <name>driver</name>
      <anonymous>false</anonymous>
    </expect-fact>
  </fixtures>
  <rules/>
  <inclusive>false</inclusive>
  <packageName>com.redhat.demo.traffic_violations</packageName>
  <imports>
    <imports/>
  </imports>
  <ksessions>
    <string>default-stateful-kie-session</string>
  </ksessions>
</scenario>